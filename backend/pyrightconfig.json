{
  "include": [
    "app"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "venv",
    ".venv",
    "env",
    "**/site-packages/**",
    "**/Lib/**",
    "**/typeshed-fallback/**",
    "**/builtins.pyi"
  ],
  "reportMissingImports": "none",
  "reportMissingModuleSource": "none",
  "reportInvalidTypeForm": "none",
  "reportUndefinedVariable": "warning",
  "pythonVersion": "3.13",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": "venv"
}
